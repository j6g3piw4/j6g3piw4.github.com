<script type="text/javascript">

$('#navbar li').click(function() {
    $(this).addClass('active').siblings('li').removeClass('active');
});


$('document').ready(function(){

	$('#top_logo , .top_news').hover(function(){
		
		
		$(this).css('opacity' , '0.6');
		$(this).css('transition' , 'all 1s');
		

	},function(){
		
		$(this).css('opacity' , '1');
		$(this).css('transition' , 'all 1s');

	})
	
	$('#page_top').click(function(){
		
		$("body").scrollTop(0);
		
	})
	
	$(window).scroll(function () {
	    var $this = $(this)
	    if ($this.scrollTop() > 120) {
	    	$('#page_top').css('visibility' , 'visible');
	    	$('#page_top').css('transition' , 'all 0.2s');
		      
	    } else {
		      $('#page_top').css('visibility' , 'hidden');
		    	$('#page_top').css('transition' , 'all 0.2s');

	    }
	});
	
	$('.more , #singlemodal , #albummodal').hover(function(){
		
		
		$(this).css('background-color' , '#fff');
		$(this).css('color' , '#000000');
		$(this).css('transition' , 'all 1s');
		

	},function(){
		

		$(this).css('background-color' , '#000000');
		$(this).css('color' , '#fff');
		$(this).css('transition' , 'all 1s');

	})
	

	
		
	$('.thumbnails').click(function(){	
		 window.open(this.href, "_blank", "toolbar=no,scrollbars=no,resizable=yes,top=50,left=200,width=1000,height=600");
		
	})
	
})



</script>
</body>
</html>
